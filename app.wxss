@import 'utils/iconfont.wxss';

page {
  font-size: 24rpx;
  min-height: 100vh;
  font-family: 'Microsoft YaHei';
}

button::after {
  border: 0;
}

.nomore, .nodata {
  padding: 30rpx 0;
  text-align: center;
  color: #bbb;
  font-size: 24rpx;
}

.nomore.tabbar, .nodata.tabbar {
  padding-bottom: 140rpx;
}

.tabbar-page {
  padding-bottom: 140rpx;
}

.full-loading {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background: #fff;
  z-index: 1500;
  overflow: hidden;
}

.full-loading .image {
  width: 250rpx;
  height: 250rpx;
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: calc(50% - 220rpx);
  background-image: url("data:image/svg+xml;base64,");
  background-size: cover;
}

.navigator-hover {
  pointer-events: none; /* 点击事件直接穿透到下层元素，即跳过这个链接或者按钮 */
  opacity: 0.8;
}

.navigator-hover {
  background: initial;
  opacity: initial;
}

.flex {
  display: flex;
}

.vh {
  visibility: hidden;
}

.text-plat-ios {
  padding: 0rpx 5rpx;
}

.text-plat {
  padding: 15rpx;
}

/* 富文本 start */

.content-wrapper image {
  max-width: 100%;
  vertical-align: bottom;
}

/* 富文本 end */

/* 超出省略 start */

.one-line-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: nowrap;
}

.two-line-ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* 超出省略 end */

/* 隐藏scroll-view滚动条 start */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 隐藏scroll-view滚动条 end */

/* more-line start */

.more-line {
  margin-top: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
  background: #ecebeb;
}

/* more-line end */

/* 需求列表（投石） start */

