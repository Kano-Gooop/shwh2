}

.work-box .work .other-line {
  margin-top: 35rpx;
  display: flex;
  justify-content: space-between;
}

.work-box .work .other-line .factory {
  font-size: 24rpx;
  color: #333;
}

.work-box .work .other-line .factory text {
}

.work-box .work .other-line .factory .amount {
  margin-left: 25rpx;
  color: #333;
  opacity: 0.7;
}

.work-box .work .other-line .agree {
}

/* 作品列表 end */

/* 点赞 start */

.agree {
  display: flex;
  align-items: flex-end;
  flex-shrink: 0;
}

.agree image {
  width: 33rpx;
  height: 34.5rpx;
  margin-right: 18rpx;
}

.agree text {
  display: inline-block;
  /* width: 75rpx; */
  width: auto;
  font-size: 25rpx;
