  height: 50rpx;
  border-radius: 50%;
}

.work-box .work .line1 .zuo .nickname {
  margin-left: 14rpx;
  line-height: 1;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.8);
}

.work-box .work .line1 .agree {
  display: flex;
  align-items: flex-end;
  flex-shrink: 0;
}

.work-box .work .line1 .agree image {
  width: 33rpx;
  height: 34.5rpx;
  margin-right: 18rpx;
}

.work-box .work .line1 .agree text {
  display: inline-block;
  /* width: 75rpx; */
  font-size: 25rpx;
  color: #999;
  line-height: 1;
}

.work-box .work .under {
  margin-top: 37rpx;
  line-height: 1;
}

.work-box .work .under .title {
  font-size: 32rpx;
  color: rgba(51, 51, 51, 0.6);
}

.work-box .work .under .desc {
  margin-top: 19rpx;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 0.6);
  line-height: 36rpx;
}

.work-box .work .under .img-box {
  margin-top: 35rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
