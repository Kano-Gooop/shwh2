}

.ac-detail .title-line .more text {
  font-size: 24rpx;
  color: #333;
  margin-right: 15rpx;
}

.ac-detail .title-line .more image {
  width: 12rpx;
  height: 23rpx;
}

.ac-detail .desc {
  margin-top: 35rpx;
  font-size: 30rpx;
  line-height: 36rpx;
  color: rgba(0, 0, 0, 0.6);
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

