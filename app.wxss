}

.ac-detail .title-line {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.ac-detail .title-line .title {
  font-size: 32rpx;
  letter-spacing: 1rpx;
  color: #333;
}

.ac-detail .title-line .more {
