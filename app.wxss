/* 退款框 start */

.refund-modal {
  padding: 10rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  height: 100vh;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}

.refund-modal .content {
  background: #fff;
  box-sizing: border-box;
  width: 80%;
  padding: 30rpx;
  border-radius: 15rpx;
}

.refund-modal .content .title {
  text-align: center;
  font-size: 30rpx;
}

.refund-modal .content textarea {
  border: 1rpx solid #ddd;
  width: calc(100% - 30rpx);
  min-height: 100rpx;
  margin-top: 23rpx;
}

.refund-modal .content .btn-box {
  display: flex;
  margin-top: 30rpx;
  justify-content: flex-end;
}

.refund-modal .content button {
  border-radius: 10rpx;
  color: #fff;
  border-radius: 10rpx;
  background: #ff9f38;
  line-height: 2;
  width: 160rpx;
  margin-right: 0;
  font-size: 26rpx;
