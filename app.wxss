@import 'utils/iconfont.wxss';

page {
  font-size: 24rpx;
  min-height: 100vh;
  font-family: 'Microsoft YaHei';
}

button::after {
  border: 0;
}

.nomore, .nodata {
  padding: 30rpx 0;
  text-align: center;
  color: #bbb;
  font-size: 24rpx;
}

.nomore.tabbar, .nodata.tabbar {
  padding-bottom: 140rpx;
}

.tabbar-page {
  padding-bottom: 140rpx;
}

.full-loading {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background: #fff;
  z-index: 1500;
  overflow: hidden;
}

.full-loading .image {
  width: 250rpx;
  height: 250rpx;
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: calc(50% - 220rpx);
  background-image: url("data:image/svg+xml;base64,");
  background-size: cover;
}

.navigator-hover {
  pointer-events: none; /* 点击事件直接穿透到下层元素，即跳过这个链接或者按钮 */
  opacity: 0.8;
}

.navigator-hover {
  background: initial;
  opacity: initial;
}

.flex {
  display: flex;
}

.vh {
  visibility: hidden;
}

.text-plat-ios {
  padding: 0rpx 5rpx;
}

.text-plat {
  padding: 15rpx;
}

/* 富文本 start */

.content-wrapper image {
  max-width: 100%;
}

/* 富文本 end */

/* 超出省略 start */

.one-line-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: nowrap;
}

.two-line-ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* 超出省略 end */

/* 隐藏scroll-view滚动条 start */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 隐藏scroll-view滚动条 end */

/* more-line start */

.more-line {
  margin-top: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
  background: #ecebeb;
}

/* more-line end */

/* 需求列表（投石） start */

.activity-list {
  margin-top: 24rpx;
}

.activity-list .activity {
  padding-bottom: 18rpx;
  border-bottom: 2rpx solid rgba(19, 27, 47, 0.07);
  margin-bottom: 44rpx;
}

.activity-list .activity .cover {
  width: 100%;
  height: 351rpx;
  vertical-align: bottom;
  border-radius: 4rpx;
}

.activity-list .activity .title {
  font-size: 32rpx;
  color: #333;
  opacity: 0.8;
  line-height: 48rpx;
  margin-top: 10rpx;
}

.activity-list .activity .info {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  line-height: 1;
  color: #999;
}

.activity-list .activity .info .people {
}

.activity-list .activity .info .people .icon {
  width: 20rpx;
  height: 23rpx;
  margin-right: 9rpx;
  vertical-align: bottom;
}

.activity-list .activity .info .people text {
}

.activity-list .activity .info .date {
}

.activity-list .activity .info .date .icon {
  width: 23rpx;
  height: 23rpx;
  margin-right: 9rpx;
  vertical-align: bottom;
}

.activity-list .activity .info .date text {
}

.activity-list .activity:last-child {
  margin-bottom: 0;
}

/* 需求列表（投石） end */

/* 作品排行榜 start */

.work-rank-list {
}

.work-rank-list .work {
  background: #fff;
  display: flex;
  padding: 20rpx;
  margin-top: 24rpx;
  box-shadow: 0 1rpx 27rpx 0 rgba(237, 237, 237, 0.75);
  border-radius: 4rpx;
}

.work-rank-list .work .cover {
  width: 190rpx;
  height: 190rpx;
  flex-shrink: 0;
}

.work-rank-list .work .info {
  flex-grow: 1;
  padding-left: 20rpx;
  box-sizing: border-box;
  width: 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.work-rank-list .work .info .diyi {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.work-rank-list .work .info .diyi .title {
  font-size: 32rpx;
  color: #666;
}

.work-rank-list .work .info .desc {
  margin-top: 14rpx;
  font-size: 28rpx;
  color: #666;
}

.work-rank-list .work .info .xia {
  /*margin-top: 14rpx;*/
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.work-rank-list .work .info .xia .left {
  display: flex;
  align-items: center;
}

.work-rank-list .work .info .xia .left image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 14rpx;
}

.work-rank-list .work .info .xia .left .nickname {
  font-size: 24rpx;
  color: #666;
}

.work-rank-list .work .info .xia .vote {
  position: relative;
  top: 10rpx;
}

.work-rank-list .work .info .xia .vote .icon {
  width: 30rpx;
  height: 26rpx;
  margin-right: 14rpx;
}

.work-rank-list .work .info .xia .vote text {
  display: inline-block;
  font-size: 26rpx;
  color: #666;
  line-height: 1;
  /* width: 65rpx; */
}

.work-rank-list .work:first-child {
  margin-top: 0;
}

/* 作品排行榜 end */

/* 创意排行榜 start */

.chuang-rank-list {
}

.chuang-rank-list .chuang {
  margin-top: 24rpx;
  background: #fff;
  padding: 20rpx;
  display: flex;
  align-items: center;
}

.chuang-rank-list .chuang .tou {
  flex-shrink: 0;
  width: 50rpx;
  text-align: center;
}

.chuang-rank-list .chuang .tou image {
  width: 45rpx;
  height: 50rpx;
}

.chuang-rank-list .chuang .cover {
  width: 60rpx;
  height: 60rpx;
  margin-left: 30rpx;
  flex-shrink: 0;
}

.chuang-rank-list .chuang .title {
  font-size: 28rpx;
  color: #333;
  opacity: 0.6;
  margin-left: 40rpx;
  flex-grow: 1;
}

.chuang-rank-list .chuang .agree {
  display: flex;
  align-items: center;
  flex-shrink: 0;
}

.chuang-rank-list .chuang .agree image {
  width: 33rpx;
  height: 34.5rpx;
  margin-right: 18rpx;
}

.chuang-rank-list .chuang .agree text {
  display: inline-block;
  /* width: 75rpx; */
  font-size: 26rpx;
  color: #999;
}

.chuang-rank-list .chuang .tou text {
}

.chuang-rank-list .chuang:first-child {
  margin-top: 0;
}

/* 创意排行榜 end */

/* 按钮 start */

.line-btn {
  background: #ffcc06;
  border-radius: 0;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0;
  font-size: 32rpx;
  color: #333;
  letter-spacing: 1rpx;
  text-align: center;
}

.line-btn[disabled]:not([type]) {
  background: #eee;
}

/* 按钮 end */

/* 众筹列表-行 start */

.chou-box .chou-line {
  margin-top: 24rpx;
  background: #fff;
}

.chou-box .chou-line .img-box {
  position: relative;
}

.chou-box .chou-line .img-box image {
  width: 100%;
  height: 351rpx;
}

.chou-box .chou-line .img-box .status {
  position: absolute;
  background: #ff684f;
  width: 96rpx;
  height: 40rpx;
  font-size: 24rpx;
  color: #fff;
  letter-spacing: 2rpx;
  top: 22rpx;
  right: 24rpx;
  border-radius: 4rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.chou-box .chou-line .info {
  padding: 24rpx;
}

.chou-box .chou-line .info .title {
  line-height: 1;
  font-size: 32rpx;
  color: #333;
  opacity: 0.8;
}

.chou-box .chou-line .info .xia {
  display: flex;
  justify-content: space-between;
  margin-top: 39rpx;
  font-size: 24rpx;
  color: #666;
}

.chou-box .chou-line .info .xia .already {
}

.chou-box .chou-line .info .xia .already .label {
}

.chou-box .chou-line .info .xia .already .price {
  color: #fac801;
}

.chou-box .chou-line .info .xia .target {
}

.chou-box .chou-line .info .xia .part-count {
}

/* 众筹列表-行 end */

/* form表单1 start */

.form1 {
}

.form1 .form-item {
  margin-bottom: 39rpx;
}

.form1 .form-item:last-child {
  margin-bottom: 0;
}

.form1 .form-item .title {
  font-size: 34rpx;
  color: #1b1b1b;
  margin-bottom: 18rpx;
}

.form1 .form-item input {
  background: #ecebeb;
  padding: 19rpx 25rpx;
  color: #4d4d4d;
  font-size: 24rpx;
  height: auto;
  line-height: 1;
}

.form1 .textarea-box {
  background: #ecebeb;
}

.form1 .textarea-box textarea {
  color: #4d4d4d;
  font-size: 24rpx;
  line-height: 1.5;
  min-height: 100rpx;
  width: 100%;
  box-sizing: border-box;
}

.form1 .textarea-box .count {
  padding: 0 24rpx 18rpx;
  color: #ccc;
  text-align: right;
  font-size: 24rpx;
}

.form1 .textarea-box .count text {
  color: #333;
}

/* form表单1 end */

/* 图片上传 start */

.up-pic {
  width: 100%;
  justify-content: center;
  margin-top: 30rpx;
}

.pic-box {
  flex-flow: wrap;
  justify-content: space-between;
  padding-right: 30rpx;
}

.ap-box {
  position: relative;
  margin-bottom: 40rpx;
}

.ap-box:nth-child(3n) {
  margin-right: 0;
}

.ap-box:nth-last-child(1), .ap-box:nth-last-child(2), .ap-box:nth-last-child(3) {
  margin-bottom: 0;
}

.add-pic {
  width: 200rpx;
  height: 200rpx;
  position: relative;
  border-radius: 4rpx;
  box-sizing: border-box;
  background: #ecebeb;
}

.add-pic .img {
  width: 100%;
  height: 100%;
}

.add-pic.plus {
  display: flex;
  justify-content: center;
  align-items: center;
}

.add-pic.plus .iconfont {
  font-size: 70rpx;
  font-weight: bold;
}

.img-de {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  position: absolute;
  right: -10px;
  top: -11rpx;
  z-index: 1;
}

/* 图片上传 end */

/* tab栏 start */

.tab-list {
  margin: 24rpx 0;
  display: flex;
}

.tab-list .tab {
  width: 0;
  flex-grow: 1;
  height: 94rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  line-height: 1;
  background-color: #ecebeb;
  color: #ccc;
}

.tab-list .tab:first-child {
  border-radius: 4rpx 4rpx 0 0;
}

.tab-list .tab:last-child {
  border-radius: 0 0 4rpx 4rpx;
}

.tab-list .tab.active {
  color: #333;
  background: #fff;
  box-shadow: 0 1rpx 60rpx 0 rgba(237, 235, 235, 0.75);
}

/* tab栏 end */

/* 作品列表 start */

.work-box {
  padding: 40rpx 24rpx 0;
}

.work-box .title {
  line-height: 1;
  font-size: 32rpx;
  color: #333;
  margin-bottom: 24rpx;
}

.work-box .work {
  margin-bottom: 24rpx;
  box-shadow: 0 1rpx 60rpx 0 rgba(237, 237, 237, 0.75);
