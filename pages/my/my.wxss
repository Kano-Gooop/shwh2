.page {
  background: #f5f5f5;
}

.banner {
  height: 275rpx;
  box-sizing: border-box;
  padding: 24rpx 24rpx 0;
  background: linear-gradient(to right, #ffc01f, #ff9734);
  position: relative;
}

.banner .head-line {
  display: flex;
  align-items: center;
}

.banner .head-line .head {
  width: 120rpx;
  height: 120rpx;
  border: 8rpx solid #ffc84e;
  border-radius: 50%;
}

.banner .head-line .info {
  margin-left: 28rpx;
  color: #333;
}

.banner .head-line .info .shang {
  margin-top: 15rpx;
  display: flex;
  align-items: center;
}

.banner .head-line .info .shang .id {
  font-size: 28rpx;
}

.banner .head-line .info .shang .role-vip {
  background: #ff6137;
  color: #fff;
  font-size: 24rpx;
  padding: 4rpx 10rpx;
  border-radius: 4rpx;
  margin-left: 10rpx;
}

.banner .head-line .info .shang .role {
  margin-right: 10rpx;
  width: 90rpx;
  height: 34rpx;
}


.banner .head-line .info .nickname {
  font-size: 34rpx;
  /*margin-top: 15rpx;*/
}

.banner .head-line .info .btn {
  border: 2rpx solid #ddd;
  padding: 12rpx 24rpx;
  border-radius: 8rpx;
  color: #333;
  font-size: 26rpx;
  background: #fffbed;
}

.banner .vip-ad {
  width: 702rpx;
  height: 95rpx;
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 8rpx 8rpx 0 0;
}

.banner .vip-ad .bg {
  vertical-align: bottom;
  width: 100%;
  height: 100%;
}

.banner .vip-ad .btn {
  position: absolute;
  right: 20rpx;
  bottom: 22rpx;
  width: 182rpx;
  height: 53rpx;
  line-height: 53rpx;
  text-align: center;
  font-size: 26rpx;
  color: #333;
}

.data-list {
  margin: 24rpx 24rpx 0;
  background: #fff;
  border-radius: 4rpx;
  height: 120rpx;
  display: flex;
}

.data-list .data {
  width: 0;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #000;
  position: relative;
}

.data-list .data::after {
  content: '';
  position: absolute;
  width: 2rpx;
  height: 60rpx;
  margin: auto;
  top: 0;
  bottom: 0;
  right: -1rpx;
  background: rgba(0, 0, 0, 0.1);
}

.data-list .data:last-child::after {
  display: none;
}

.data-list .data .amount {
  font-size: 38rpx;
}

.data-list .data .title {
  margin-top: 16rpx;
  font-size: 24rpx;
}

.order-box {
  margin: 24rpx 24rpx 0;
  border-radius: 4rpx;
  background: #fff;
}

.order-box .title-line {
  padding: 24rpx;
  display: flex;
  align-self: flex-end;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.03);
  justify-content: space-between;
}

.order-box .title-line .title {
  font-size: 34rpx;
  color: #333;
}

.order-box .title-line .all {
  display: flex;
  align-items: center;
}

.order-box .title-line .all .text {
  color: #666;
  font-size: 24rpx;
  margin-right: 11rpx;
}

.order-box .title-line .all .you {
  width: 14rpx;
  height: 24rpx;
}

.order-box .status-box {
  padding: 43rpx 24rpx 24rpx;
  display: flex;
  justify-content: space-between;
}

.order-box .status-box .status {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.order-box .status-box .status .icon {
}

.order-box .status-box .status .icon.i1 {
  width: 52rpx;
  height: 46rpx;
  margin-bottom: 27rpx;
}

.order-box .status-box .status .icon.i2 {
  width: 50rpx;
  height: 46rpx;
  margin-bottom: 27rpx;
}

.order-box .status-box .status .icon.i3 {
  width: 50rpx;
  height: 48rpx;
  margin-bottom: 25rpx;
}

.order-box .status-box .status .icon.i4 {
  width: 43rpx;
  height: 48rpx;
  margin-bottom: 25rpx;
}

.order-box .status-box .status .icon.i5 {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 23rpx;
}

.order-box .status-box .status .text {
  font-size: 26rpx;
  color: #333;
}

.service-box {
  margin: 24rpx 24rpx 0;
  background: #fff;
}

.service-box .title {
  padding: 24rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.03);
  font-size: 34rpx;
  color: #333;
}

.service-box .service-list {
  padding: 43rpx 24rpx 24rpx;
  display: flex;
  justify-content: space-between;
}

.service-list .service {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 104rpx;
  padding: 0;
  margin: 0;
  box-shadow: none;
  line-height: 1;
  font-weight: normal;
  background: transparent;
  border-radius: 0;
}

.service-list .service .icon {
}

.service-list .service .icon.i1 {
  width: 30rpx;
  height: 28rpx;
  margin-bottom: 27rpx;
}

.service-list .service .icon.i2 {
  width: 30rpx;
  height: 34rpx;
  margin-bottom: 21rpx;
}

.service-list .service .icon.i3 {
  width: 30rpx;
  height: 34rpx;
  margin-bottom: 21rpx;
}

.service-list .service .icon.i4 {
  width: 38rpx;
  height: 36rpx;
  margin-bottom: 19rpx;
}

.service-list .service .text {
  font-size: 26rpx;
  color: #333;
}