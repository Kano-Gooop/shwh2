.page {
    padding-top:1rpx;
    box-sizing:border-box;
    position: fixed;
    left: 0;
    top: 0;
    width: 100vw;
    height: 100vh;
}

.bg {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    vertical-align: bottom;
}

.main {
    position: relative;
    z-index: 1;
}

.main > image {
    width:100rpx;
    height:85rpx;
    margin:40rpx auto 0;
    display:block;
}

.white-box {
    background:#fcfcfc;
    width:90%;
    margin:30rpx auto 0;
    padding-bottom:1rpx;
}

.white-box .part1 {
    width:100%;
}

.white-box .box-xia {
    padding:0 35rpx;
}

.white-box .box-xia .block {
    border:2rpx solid #313131;
    padding-bottom:20rpx;
}

.white-box .box-xia .block > .title {
    text-align:center;
    font-size:32rpx;
    margin-top:14rpx;
}

.white-box .box-xia .li {
    margin-top:15rpx;
}

.white-box .box-xia .li > view {
    box-sizing:border-box;
}

.white-box .box-xia .li .titles {
    display:flex;
    box-sizing:border-box;
    justify-content:space-between;
    padding:0 20rpx;
}

.white-box .box-xia .li .title-box {
    display: flex;
    justify-content: center;
    width: 0;
    flex-grow: 1;
}

.white-box .box-xia .li .title {
    background:#dadada;
    padding:7rpx 3rpx;
    border-radius:10rpx;
}

.white-box .box-xia .li .shu {

}

.white-box .box-xia .li scroll-view {
    height:136rpx;
    padding:10rpx 20rpx 0;
    box-sizing:border-box;
    text-align:center;
}

.white-box .box-xia .li scroll-view .left {
    display:inline-block;
    height:40rpx;
    line-height:40rpx;
    width:50%;
    box-sizing:border-box;
    text-align:center;

}

.white-box .box-xia .li scroll-view .right {
    display:inline-block;
    height:40rpx;
    line-height:40rpx;
    width:50%;
    box-sizing:border-box;
    text-align:center;
}

.white-box .box-xia .li scroll-view .right view {
    color: red;
}

.white-box .box-xia .hr {
    margin:38rpx 0 30rpx;
    width:100%;
}

.white-box .box-xia .gui {
    line-height:1.5;
    padding:15rpx 15rpx 0;
}

.white-box .to-qrcode {
    background:#e5e3e4;
    width:250rpx;
    display:block;
    margin:70rpx auto 60rpx;
    text-align:center;
    line-height:59rpx;
    border:1rpx solid #aaa;
    font-size:26rpx;
    border-radius:45rpx;
}

