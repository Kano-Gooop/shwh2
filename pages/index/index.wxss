.page {
  padding: 24rpx 24rpx 50rpx;
  background: #fafafa;
}

.page-top {
  text-align: center;
  line-height: 64rpx;
}

.tou-swiper {
  height: 320rpx;
  /* margin-top: -18rpx; */
}

.tou-swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
  position: absolute;
  bottom: -10rpx;
}

.page-top .feng-bian {
  position: relative;
  margin-left: -14rpx;
  margin-right: -14rpx;
  margin-top: -14rpx;
  z-index: 1;
  display: none;
}

.page-top .feng-bian image {
  width: 100%;
  height: 64rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.page-top .feng-bian text {
  position: relative;
  z-index: 1;
  font-size: 28rpx;
  color: #2d2e31;
}

.gong-gao {
  display: flex;
  margin-top: 30rpx;
  background: #fff;
  border-radius: 8rpx;
}

.gong-gao .icon-box {
  flex-shrink: 0;
  width: 86rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
  box-shadow: 0 1rpx 20rpx 0 rgba(229, 229, 229, 0.65);
}

.gong-gao .tiao {
  flex-shrink: 0;
  width: 16rpx;
  height: 60rpx;
}

.gong-gao .icon-box image {
  width: 38rpx;
  height: 32rpx;
}

.gong-gao swiper {
  width: 0;
  flex-grow: 1;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 11rpx;
}

.gong-gao swiper swiper-item {
  color: #1a1a1a;
  font-size: 26rpx;
}

.h-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 64rpx 0 50rpx;
}

.h-title image {
  width: 193rpx;
  height: 38rpx;
}

.h-title .more {
  font-size: 22rpx;
  color: #999;
}

.kingkong {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 30rpx 0 0;
  background: #f5f5f5;
  padding: 40rpx 0;
  box-shadow: 0 1rpx 60rpx 0 rgba(211, 211, 211, 0.6);
}

.kingkong navigator {
  display: block;
  width: 100rpx;
  text-align: center;
}

.kingkong navigator image {
  vertical-align: bottom;
}

.kingkong .musemu {
  width: 96rpx;
  height: 110rpx;
}

.kingkong .designer {
  width: 97rpx;
  height: 107rpx;
}

.kingkong .factory {
  width: 78rpx;
  height: 109rpx;
}

.kuai-dan {
  margin-top: 52rpx;
}

.kuai-dan .shang {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 24rpx;
  border-bottom: 2rpx solid #eee;
}

.kuai-dan .shang .wk {
  width: 110rpx;
  height: 30rpx;
}

.kuai-dan .shang .more {
  font-size: 26rpx;
  opacity: 0.6;
  color: #1a1a1a;
}

.kuai-dan .list {
  margin-top: 39rpx;
}

.kuai-dan .list .dan {
  margin-bottom: 23rpx;
  display: flex;
  align-items: center;
  line-height: 1;
}

.kuai-dan .list .dan::before {
  content: '';
  width: 6rpx;
  height: 6rpx;
  border-radius: 50%;
  margin-right: 17rpx;
  background: #313131;
  flex-shrink: 0;
}

.kuai-dan .list .dan:last-child {
  margin-bottom: 0;
}

.kuai-dan .list .dan navigator {
  opacity: 0.8;
  color: #1a1a1a;
  font-size: 26rpx;
  flex-grow: 1;
}

.activity-box {
  margin-top: 47rpx;
}

.rank-box {
  margin-top: 50rpx;
}

.chuang-rank-list .tag-line {
  margin-top: 24rpx;
}

.chou-box {
  margin-top: 48rpx;
}

.chou-box .look-forward {
  padding: 15rpx;
  background: #eee;
  font-size: 30rpx;
  color: #666;
}

.chou-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.chou-list .chou {
  width: 340rpx;
  margin-top: 24rpx;
  background: #fff;
  border-radius: 4rpx;
  box-shadow: 0 1rpx 27rpx 0 rgba(237, 237, 237, 0.75);
  overflow: hidden;
}

.chou-list .chou .cover {
  width: 100%;
  height: 170rpx;
}

.chou-list .chou:nth-child(2n) {
  margin-right: 0;
}

.chou-list .chou .info {
  padding: 20rpx;
}

.chou-list .chou .info .title {
  color: #333;
  opacity: 0.8;
  font-size: 26rpx;
}

.chou-list .chou .info .bottom {
  margin-top: 24rpx;
  font-size: 20rpx;
  color: #666;
  display: flex;
  justify-content: space-between;
}

.chou-list .chou .info .bottom .already {
}

.chou-list .chou .info .bottom .part-count {
}
