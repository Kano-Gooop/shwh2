page {
    background: #f6f7f9;
    box-sizing: border-box;
}

.cate-wrapper {
    margin-top: 20rpx;
    display: flex;
    overflow: visible;
}

.cate-wrapper .one-level {
    width: 200rpx;
    flex-shrink: 0;
    background: #fff;
    margin-top: -20rpx;
    box-sizing: border-box;
    height: 100vh;
}

.cate-wrapper .one-level .scroll {
}

.cate-wrapper .one-level .item {
    font-size:28rpx;
    padding:20rpx 0 20rpx 30rpx;
    color:#3d3d3d;
}

.cate-wrapper .one-level .item.active {
    background: #f6f7f9;
}

.cate-wrapper .two-level {
    background: #fff;
    flex-grow: 1;
    margin-left: 20rpx;
    padding: 30rpx 20rpx;
    box-sizing: border-box;
    height: calc(100vh - 20rpx);
}

.cate-wrapper .two-level .scroll {
    display: flex;
    flex-wrap: wrap;
}

.cate-wrapper .two-level .scroll .no-pro {
    width:100%;
    text-align:center;
    color:#666;
}

.cate-wrapper .two-level .item {
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 33.3%;
    margin-bottom: 25rpx;
}

.cate-wrapper .two-level image {
    width:130rpx;
    height:150rpx;
}

.cate-wrapper .two-level .title {
    color: #999;
    margin-top: 10rpx;
}